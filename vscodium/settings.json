{
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "vscode-icons",
    "git.autofetch": true,
    "vsicons.dontShowNewVersionMessage": true,
    "git.confirmSync": false,
    "cSpell.userWords": [
        "astype",
        "bandpass",
        "btype",
        "cheby",
        "Cliping",
        "CPPFLAGS",
        "cuda",
        "dioption",
        "dnntts",
        "doas",
        "downsampling",
        "finfo",
        "firwin",
        "Fumiyoshi",
        "irfft",
        "jfloatwavio",
        "Koguchi",
        "lfilter",
        "librosa",
        "LMDE",
        "MATANO",
        "matplotlib",
        "MONOPHONE",
        "Morise",
        "mreccorpus",
        "mypy",
        "nbit",
        "nbits",
        "ndarray",
        "nepochs",
        "nnmnkwii",
        "noisereduce",
        "numpy",
        "omegaconf",
        "PYPI",
        "pyproject",
        "Qiita",
        "quantile",
        "rfft",
        "rfftfreq",
        "scipy",
        "setuptools",
        "sosfilt",
        "soundfile",
        "soxr",
        "Tacotron",
        "temurin",
        "tqdm",
        "ttslearn",
        "unsqueeze",
        "utternace",
        "vsed",
        "xfce"
    ],
    "liveServer.settings.donotVerifyTags": true,
    "editor.fontSize": 15,
    "editor.fontFamily": "JetBrains Mono, Menlo, Monaco, 'Courier New', monospace",
    "liveServer.settings.donotShowInfoMsg": true,
    "cSpell.customDictionaries": {},
    "editor.fontLigatures": true,
    "files.associations": {
        "CHANGELOG": "plaintext",
        "*.bash": "shellscript"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "git.openRepositoryInParentFolders": "always",
    "editor.detectIndentation": false,
    "javascript.inlayHints.variableTypes.enabled": true,
    "extensions.supportUntrustedWorkspaces": {},
    "MATLAB.installPath": "/Applications/MATLAB_R2023b.app",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": "bold",
    "editor.fontWeight": "bold",
    "java.checkstyle.configuration": "google_checks.xml",
    "editor.stickyScroll.enabled": true,
    "workbench.editorAssociations": {
        "*.lock": "default",
        "*.mlapp": "default"
    },
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "redhat.telemetry.enabled": true,
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "editor.unicodeHighlight.nonBasicASCII": true,
    "terminal.integrated.commandsToSkipShell": [
        "matlab.interrupt"
    ],
    "csv-edit.readOption_hasHeader": "true",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "mypy-type-checker.showNotifications": "onError",
    "diffEditor.ignoreTrimWhitespace": false,
    "git.replaceTagsWhenPull": true,
    "svg.preview.mode": "img",
    "workbench.productIconTheme": "bongocat",
    "gitlens.codeLens.enabled": false,
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.inheritEnv": false,
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv",
    "editor.formatOnSave": true
}
